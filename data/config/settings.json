{

	##############################################################
	# These settings CAN NOT be changed by mods.
	# You can still change them yourself, though.
	##############################################################
	
	"vsync":true,
	"fps":60, # fps only used if vsync is false
	"devMode":false,
	"godMode":false, # 100x damage dealt, no damage taken
	"playtestingMode":false, # turns off some debug flags while in dev mode; doesn't do anything if not in dev mode.
	
	# if set to true, will print which rules are checked, which conditions pass/fail, etc
	# also likely to add a noticeable delay to when interaction options are selected
	# can also be toggled on/off in code via DebugFlags.PRINT_RULES_DEBUG_INFO
	"printRulesDebugInfo":false,
	
	"queryMaxAASamples":true,
	"maxAASamples":16, # selectable value is limited by this
	"aaSamplesOverride":-1, # >= 0 overrides value selected in launcher; required to use AA samples with legacyLauncher
	"screenScaleOverride":0, # > 0 overrides UI scaling value from in-game settings 
	"enableLauncherWarnings":true,
	
	
	# experimental feature; can cause problems such as invisible ships in combat and mouse-panning issues
	# but only on some PCs
	"allowJVMRestartFromLauncher":false,
	"allowJVMRestartFromLauncherInDevMode":false,
	
	# these only have any effect if allowJVMRestartFromLauncher is set to true
	"baseMemoryRequired":1024,
	"maxMemoryFractionOfTotal":0.5,
	"maxMemoryMB":6144,
	
	
	"doRAMandVRAMChecksWhenRunningWithMods":true, # will periodically check if ram/vram is low and warn the player
	"memoryCheckVerbose":false,
	
	"colorblindMode":false,
	"undecoratedWindow":false,
	"alwaysUndecoratedAtFullscreen":true,
	"enableSoundFilters":true,
	#"resolutionOverride":"1024x768",
	#"refreshRateOverride":60,
	#"forceAspectRatio":"1.3333",
	#"windowLocationX":0,
	#"windowLocationY":0,
	
	"showEnemyWaypoints":false,
	
	"allowForceQuitInIronMode":true, # whether Alt-F4 and similar works when playing campaign in iron mode
	
	"useDefaultMouseCursor":false,
	
	"allowAnyJavaVersion":true,
	
